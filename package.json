{
  "name": "@visiosto/cave",
  "version": "0.1.0",
  "description": "Visiosto's internal OpenGraph image generator",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{?([cm])[jt]s?(x),json,md?(x),html,gitignore,env}\"",
    "format:test": "prettier --check \"**/*.{?([cm])[jt]s?(x),json,md?(x),html,gitignore,env}\"",
    "lint": "concurrently 'npm:lint:*'",
    "lint:js": "eslint .",
    "lint:ts": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visiosto/cave.git"
  },
  "author": "Visiosto oy <visiosto@visiosto.fi>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/visiosto/cave/issues"
  },
  "homepage": "https://github.com/visiosto/cave#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "puppeteer": "^21.6.1"
  }
}
